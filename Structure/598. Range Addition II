Time O(m * n * ops.size())
int maxCount(int m, int n, vector<vector<int>>& ops) {
        int maxV = 0;
        int count = 0;
        if(ops.size() == 0) return m * n;
        vector<vector<int>> M(m, vector<int>(n, 0));
        for(vector<int> o : ops){
            for(int i = 0; i < o[0]; i++){
                for(int j = 0; j < o[1]; j++){
                    M[i][j]++;
                    if(M[i][j] > maxV){
                        maxV = M[i][j];
                        count = 1;
                    }else if(M[i][j] == maxV){
                        count ++;
                    }
                }
            }
        }
        return count;
}
