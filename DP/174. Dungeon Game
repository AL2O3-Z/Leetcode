int calculateMinimumHP(vector<vector<int>>& dungeon) {
        int m = dungeon.size();
        if(!m) return 0;
        int n = dungeon[0].size();
        vector<vector<int>> dp(m+1, vector<int>(n+1, INT_MAX));
        dp[m][n-1] = 1;
        dp[m-1][n] = 1;
        for(int i = m-1; i >= 0; i--){
            for(int j = n-1; j >= 0; j--){
                int tmp = min(dp[i+1][j], dp[i][j+1]) - dungeon[i][j];
                dp[i][j] = tmp > 0 ? tmp : 1;
            }
        }
        return dp[0][0];
    }
    //倒推，使骑士救完公主后跑出游戏区域，生命值至少为1.
